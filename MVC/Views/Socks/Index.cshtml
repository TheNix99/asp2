@model IEnumerable<Socks>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@

@{
    ViewData["Title"] = "Ponožky";
}

<h1>Ponožky</h1>

@foreach (var sock in Model)
{
    <a asp-action="GetById" asp-route-id="@sock.Id" asp-controller="Socks">
        <p>
            @sock.Brand - @sock.Size - @sock.Price Kč - skladem: @sock.OnStock ks
        </p>
    </a>
}
